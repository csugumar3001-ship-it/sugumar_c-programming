#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    
    scanf("%d", &n);
    
    int sum = 0, x;
    
    for(int i = 0; i < n - 1; i++) {
        scanf("%d", &x);
        sum += x;
    }
    
  
    int total = n * (n + 1) / 2;
    
    int missing = total - sum;
    
    printf("%d", missing);
    
    return 0;
}


